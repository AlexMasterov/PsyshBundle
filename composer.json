{
  "name": "alexmasterov/psysh-bundle",
  "description": "Bundle to integrate REPL PsySH with Symfony projects",
  "type": "symfony-bundle",
  "license": "MIT",
  "homepage": "https://github.com/AlexMasterov/psysh-bundle",
  "authors": [{
    "name": "Alex Masterov",
    "email": "alex.masterow@gmail.com"
  }],
  "keywords": [
    "psysh",
    "symfony",
    "bundle"
  ],
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "AlexMasterov\\PsyshBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AlexMasterov\\PsyshBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "7.1 - 7.3",
    "psy/psysh": "0.8 - 0.9",
    "symfony/config": "3.3 - 4.0",
    "symfony/dependency-injection": "3.3 - 4.0",
    "symfony/http-kernel": "3.3 - 4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  }
}
